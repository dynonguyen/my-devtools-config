{
	"manifest_version": 3,
	"name": "Dyno Command Palette",
	"version": "1.0.0",
	"description": "A command palette to navigate in Google Chrome faster âš¡",
	"icons": {
		"16": "assets/logo.png",
		"32": "assets/logo.png",
		"64": "assets/logo.png",
		"128": "assets/logo.png"
	},
	"permissions": ["bookmarks"],

	"action": { "default_icon": "assets/logo.png" },

	"web_accessible_resources": [
		{ "resources": ["assets/*"], "matches": ["<all_urls>"] }
	],
	"content_scripts": [
		{
			"matches": ["http://localhost:8888/*"],
			"js": ["content/index.js"],
			"css": ["content/index.css"]
		}
	],
	"background": { "service_worker": "background/index.js", "type": "module" },

	"commands": {
		"_execute_action": {
			"suggested_key": { "default": "Ctrl+Shift+P", "mac": "Command+Shift+P" },
			"description": "Open command palette"
		}
	}
}
