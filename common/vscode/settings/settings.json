{
	// ------------------------------------------------------------
	// ------------------------ BUILT-IN ------------------------
	// ------------------------------------------------------------
	// Editor
	"editor.acceptSuggestionOnCommitCharacter": false,
	"editor.accessibilitySupport": "off",
	"editor.autoIndent": "advanced",
	"editor.bracketPairColorization.enabled": true,
	"editor.codeLens": true,
	"editor.colorDecorators": true,
	"editor.columnSelection": false,
	"editor.cursorBlinking": "expand",
	"editor.cursorWidth": 2,
	"editor.defaultColorDecorators": "never",
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.fastScrollSensitivity": 10,
	"editor.foldingImportsByDefault": false,
	"editor.fontLigatures": true,
	"editor.fontWeight": 500,
	"editor.formatOnSave": true,
	"editor.largeFileOptimizations": true,
	"editor.lineNumbers": "relative",
	"editor.mouseWheelZoom": true,
	"editor.occurrencesHighlight": "off",
	"editor.renderControlCharacters": false,
	"editor.quickSuggestions": { "strings": true },
	"editor.renderLineHighlight": "all",
	"editor.renderWhitespace": "none",
	"editor.smoothScrolling": true,
	"editor.suggest.insertMode": "replace",
	"editor.suggest.preview": true,
	"editor.suggestSelection": "first",
	"editor.tabSize": 2,
	"editor.unicodeHighlight.ambiguousCharacters": false,
	"editor.unicodeHighlight.includeComments": false,
	"editor.unicodeHighlight.invisibleCharacters": false,
	"editor.unicodeHighlight.nonBasicASCII": false,
	"editor.wordWrap": "on",
	"editor.cursorSurroundingLines": 10,
	"editor.snippetSuggestions": "inline",
	"editor.linkedEditing": false,
	"editor.minimap.enabled": false,
	"editor.codeActionsOnSave": {
		"source.organizeImports": "explicit"
		// "source.addMissingImports": "explicit",
		// "source.removeUnused": "explicit"
	},
	"editor.inlineSuggest.enabled": true,
	"editor.inlineSuggest.syntaxHighlightingEnabled": false,
	"editor.fontFamily": "Fira Code",
	"editor.fontSize": 16,
	"diffEditor.hideUnchangedRegions.enabled": true,

	// Explorer
	"explorer.confirmPasteNative": false,
	"explorer.compactFolders": false,
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,

	// Window
	"window.commandCenter": true,
	"window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}",
	"window.titleBarStyle": "custom",
	"window.newWindowProfile": "MacOS",

	// Workbench
	"workbench.editor.enablePreview": false,
	"workbench.list.smoothScrolling": true,
	"workbench.editor.pinnedTabSizing": "compact",
	"workbench.startupEditor": "none",
	"workbench.colorTheme": "Dyno Cute",
	"workbench.iconTheme": "material-icon-theme",
	"workbench.editor.empty.hint": "hidden",

	// Languages
	"[css]": { "editor.matchBrackets": "always" },
	"[html]": { "editor.matchBrackets": "never" },
	"[svelte]": { "editor.defaultFormatter": "svelte.svelte-vscode" },
	"[java]": { "editor.defaultFormatter": "redhat.java", "editor.tabSize": 4 },
	"[nginx]": { "editor.defaultFormatter": "ahmadalli.vscode-nginx-conf" },
	"[php]": {
		"editor.defaultFormatter": "bmewburn.vscode-intelephense-client",
		"editor.detectIndentation": true,
		"editor.insertSpaces": true,
		"editor.tabSize": 4
	},
	// "[proto3]": { "editor.defaultFormatter": "zxh404.vscode-proto3" },
	"[scala]": { "editor.defaultFormatter": "scalameta.metals" },
	"[go]": { "editor.defaultFormatter": "golang.go" },
	"[snippets]": { "editor.defaultFormatter": "vscode.json-language-features" },
	"[shellscript]": {
		"editor.defaultFormatter": "foxundermoon.shell-format"
	},
	"[ignore]": {
		"editor.defaultFormatter": "foxundermoon.shell-format"
	},
	"[dotenv]": {
		"editor.defaultFormatter": "foxundermoon.shell-format"
	},
	"[properties]": {
		"editor.defaultFormatter": "foxundermoon.shell-format"
	},
	"[python]": {
		"editor.defaultFormatter": "ms-python.black-formatter",
		"editor.tabSize": 4
	},

	// Typescript
	"typescript.tsserver.log": "off",
	// "typescript.preferences.importModuleSpecifier": "project-relative",

	// Emmet
	"emmet.includeLanguages": {
		"markdown": "html",
		"handlebar": "html",
		"postcss": "css"
	},
	"emmet.triggerExpansionOnTab": true,
	"emmet.showSuggestionsAsSnippets": false,

	// File browser
	"files.associations": {
		"*.min.*": "plaintext",
		"*.applescript": "shellscript",
		"*.svelte": "html"
	},
	"files.exclude": {
		"**/node_modules": true
	},
	"files.watcherExclude": {
		"**/.ammonite": true,
		"**/.bloop": true,
		"**/.metals": true
	},
	"search.exclude": {
		"**/node_modules": true,
		"**/build": true,
		"**/dist": true,
		"**/.git": true,
		"**/.vercel": true,
		"**/.vscode": true,
		"**/*.min.*": true,
		"**/.env": false,
		"**/yarn.lock": true,
		"**/pnpm-lock.yaml": true
	},

	// Terminal
	"terminal.external.linuxExec": "warp-terminal",
	"terminal.external.osxExec": "Warp.app",
	"terminal.integrated.cursorStyle": "line",
	"terminal.integrated.defaultProfile.linux": "zsh",
	"terminal.integrated.fontFamily": "MesloLGS NF",
	// "terminal.integrated.fontSize": 14,
	"terminal.integrated.shellIntegration.enabled": true,
	"terminal.integrated.tabs.enabled": false,
	"terminal.integrated.suggest.enabled": true,
	"terminal.integrated.suggest.selectionMode": "always",

	// Git
	"git.enableSmartCommit": true,
	"git.decorations.enabled": true,
	"git.autofetch": true,
	"git.openRepositoryInParentFolders": "always",

	// Others
	"keyboard.dispatch": "keyCode",
	"breadcrumbs.icons": true,
	"css.lint.unknownAtRules": "ignore",
	"comments.visible": false,
	"security.promptForLocalFileProtocolHandling": false,
	"extensions.experimental.affinity": {
		"vscodevim.vim": 1
	},

	// ------------------------------------------------------------
	// ------------------------ Extensions ------------------------
	// ------------------------------------------------------------
	// Vim
	"vim.autoindent": true,
	"vim.autoSwitchInputMethod.defaultIM": "normal",
	"vim.easymotion": true,
	"vim.easymotionMarkerForegroundColorOneChar": "#ff6bcb",
	"vim.foldfix": true,
	"vim.highlightedyank.enable": false,
	"vim.history": 200,
	"vim.ignorecase": false,
	"vim.incsearch": false,
	"vim.joinspaces": false,
	"vim.leader": "<space>",
	"vim.mouseSelectionGoesIntoVisualMode": true,
	"vim.surround": true,
	"vim.useCtrlKeys": true,
	"vim.useSystemClipboard": true,

	"vim.handleKeys": {
		"<C-a>": false,
		"<C-b>": false,
		"<C-c>": false,
		"<C-d>": false,
		"<C-f>": false,
		"<C-h>": false,
		"<C-l>": false,
		"<C-v>": false,
		"<C-w>": false,
		"<C-x>": false,
		"<C-n>": false,
		"<C-o>": false,
		"<C-p>": false,
		"<D-c>": false,
		"<D-v>": false
	},

	"vim.normalModeKeyBindingsNonRecursive": [
		// ================ Vim command mapping
		// Chi xoa, khong cat. Su dung leader de xoa + cat
		{ "before": ["d"], "after": ["\"", "_", "d"] },
		{ "before": ["<leader>", "d"], "after": ["d"] },
		{ "before": ["x"], "after": ["\"", "_", "x"] },

		// Chi xoa, khong cat. Su dung leader de xoa + cat -> insert mode
		{ "before": ["c"], "after": ["\"", "_", "c"] },
		{ "before": ["<leader>", "c"], "after": ["c"] },

		// Di chuyen nhanh len/xuong 10 dong
		{ "before": ["D"], "after": ["1", "0", "j"] },
		{ "before": ["U"], "after": ["1", "0", "k"] },

		// Dua con tro ve truoc/sau cua 1 tag
		{ "before": ["t", "i"], "after": ["f", ">", "i"] },
		{ "before": ["t", "a"], "after": ["f", ">", "a"] },

		// Xoa tat ca noi dung trong 1 tag -> insert mode
		{ "before": ["t", "c"], "after": ["f", ">", "l", "c", "i", "t"] },

		// Toggle eazy motion
		{ "before": ["f", "f"], "after": ["<leader>", "<leader>", "2", "s"] },

		// Fold all
		{ "before": ["Z", "Z"], "after": ["z", "M", "z", "o"] },

		// Di vao ben trong scope {} gan nhat trong dong
		{ "before": ["B"], "after": ["F", "{", "a"] },

		// ================ Vscode Command mapping
		// Di chuyen qua lai giua cac editors
		{
			"before": ["<leader>", "l"],
			"commands": ["workbench.action.navigateRight"]
		},
		{
			"before": ["<leader>", "h"],
			"commands": ["workbench.action.navigateLeft"]
		},

		// Toggle suggestions
		{ "before": ["<C-space>"], "commands": ["editor.action.triggerSuggest"] },

		// Redo
		{ "before": ["<C-r>"], "commands": ["redo"] },

		// Unfold all
		{ "before": ["z", "O"], "commands": ["editor.unfoldAll"] },
		{ "before": ["z", "r"], "commands": ["editor.unfoldRecursively"] },

		// Xoa surround brackets
		{ "before": ["d", "s", "s"], "commands": ["editor.action.removeBrackets"] },

		// Show definition preview
		{
			"before": ["g", "h"],
			"commands": ["editor.action.showDefinitionPreviewHover"]
		},

		// Find all references
		{ "before": ["g", "r"], "commands": ["references-view.findReferences"] },

		// Tab
		{ "before": ["<Tab>"], "commands": ["editor.action.indentLines"] },
		{ "before": ["<S-Tab>"], "commands": ["editor.action.outdentLines"] }
	],

	"vim.visualModeKeyBindingsNonRecursive": [
		{ "after": ["\"", "_", "d"], "before": ["d"] },
		{ "after": ["x"], "before": ["<leader>", "d"] },
		{ "before": ["<Tab>"], "commands": ["editor.action.indentLines"] },
		{ "before": ["<S-Tab>"], "commands": ["editor.action.outdentLines"] }
	],

	// Prettier
	"prettier.arrowParens": "avoid",
	"prettier.jsxSingleQuote": true,
	"prettier.printWidth": 120,
	"prettier.singleQuote": true,
	"prettier.tabWidth": 2,
	"prettier.trailingComma": "all",
	"prettier.useTabs": true,

	// Git lens
	"gitlens.currentLine.enabled": false,
	"gitlens.statusBar.enabled": true,
	"gitlens.codeLens.enabled": false,

	// Dyno File Utils
	"dynoFileUtils.confirmDelete": false,
	"dynoFileUtils.expandSeparator": ",",
	"dynoFileUtils.openFile": true,
	"dynoFileUtils.separator": " ",
	"dynoFileUtils.folderExclude": [
		"node_modules",
		".git",
		"build",
		"dist",
		"bin",
		"obj",
		".",
		"target",
		"database",
		"db",
		"vendor",
		"deprecated",
		"volumes"
	],

	// Auto rename tag
	"auto-rename-tag.activationOnLanguage": [
		"php",
		"html",
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"xml",
		"mdx",
		"vue"
	],

