	// Error lens
	"errorLens.delay": 2000,
	"errorLens.enabledDiagnosticLevels": ["error", "warning"],
	"errorLens.followCursor": "activeLine",
	"errorLens.gutterIconsEnabled": true,
	"errorLens.margin": "12px",
	"errorLens.messageMaxChars": 200,

	// cSpell checker
	"cSpell.ignorePaths": ["**/vi.ts", "**/settings.json"],
	"cSpell.userWords": ["dynonguyen", "dyno"],
	"cSpell.textDecorationThickness": "1px",
	"cSpell.textDecorationStyle": "wavy",

	// Material Icon Theme
	"material-icon-theme.hidesExplorerArrows": true,
	"material-icon-theme.activeIconPack": "react",

	// Iconify
	"iconify.delimiters": [":", "-"],
	"iconify.annotations": true,
	"iconify.languageIds": ["javascriptreact", "typescriptreact"],

	// Github copilot
	"github.copilot.nextEditSuggestions.enabled": false,
	"github.copilot.enable": { "markdown": true },
	"github.copilot.chat.commitMessageGeneration.instructions": [
		{ "file": ".cz-config.js" },
		{ "file": ".cz-config.cjs" },
		{ "file": "commitlint.config.js" },
		{ "file": "commitlint.config.cjs" }
