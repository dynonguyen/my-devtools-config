		"rgba(38, 70, 83,0.25)",
		"rgba(42, 157, 143,0.1)",
		"rgba(233, 196, 106,0.1)",
		"rgba(244, 162, 97,0.1)",
		"rgba(231, 111, 81,0.1)",
		"rgba(252, 163, 17,0.1)"
