{
	"name": "Dyno Dark Theme",
	"type": "dark",
	"colors": {
		"activityBar.activeBorder": "#ee4ecc",
		"activityBar.background": "#27293d",
		"activityBarBadge.background": "#ee4ecc",

		"breadcrumb.background": "#151522",
		"breadcrumb.foreground": "#8f8f98",

		"button.background": "#bf50f0",
		"button.hoverBackground": "#cf73f3",

		"dropdown.background": "#151522",

		"editor.background": "#1e1e2e",
		"editor.foreground": "#e5e5e5",
		"editor.lineHighlightBorder": "#1a1a29",
		"editor.selectionBackground": "#2f3646",
		"editor.selectionHighlightBackground": "#2f3646",
		"editor.snippetTabstopHighlightBackground": "#243444",
		"editorCursor.foreground": "#4ece58",
		"editorGroupHeader.tabsBackground": "#1e2031",
		"editorLineNumber.activeForeground": "#faa72a",
		"editorLineNumber.foreground": "#4a4a5c",
		"editorOverviewRuler.border": "#1e1e2e",
		"editorWidget.background": "#151522",
		"editorWidget.border": "#151522",
		"errorForeground": "#fe4e6e",

		"focusBorder": "#1e1e2e",
		"foreground": "#e5e5e5",

		"input.background": "#27293d",

		"list.activeSelectionBackground": "#252e47",
		"list.focusBackground": "#27293d",
		"list.focusForeground": "#ffffff",
		"list.hoverBackground": "#27293d",

		"notifications.background": "#151522",
		"panel.background": "#1e1e2e",
		"panel.border": "#151522",
		"panelTitle.activeBorder": "#ee4ecc",

		"scrollbar.shadow": "#131319",
		"scrollbarSlider.activeBackground": "#303042",
		"scrollbarSlider.background": "#232333",
		"scrollbarSlider.hoverBackground": "#262636",
		"sideBar.background": "#1e1e2e",
		"sideBar.border": "#151522",
		"sideBarSectionHeader.background": "#1e1e2e",
		"statusBar.background": "#27293d",
		"statusBar.debuggingBackground": "#27293d",
		"statusBar.noFolderBackground": "#27293d",

		"tab.activeBackground": "#151522",
		"tab.activeBorderTop": "#ee4ecc",
		"tab.inactiveBackground": "#283046",
		"tab.unfocusedHoverBackground": "#272746",
		"terminal.background": "#1e1e2e",
		"textLink.activeForeground": "#39daeb",
		"textLink.foreground": "#00b4e4",
		"titleBar.activeBackground": "#27293d"
	},
	"tokenColors": [
		{
			"name": "Html Text",
			"scope": ["text.html.derivative"],
			"settings": {
				"foreground": "#e5e5e5"
			}
		},
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#4d597e"
			}
		},
		{
			"name": "Block Comment",
			"scope": ["comment.block"],
			"settings": {
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "Variables",
			"scope": ["variable", "string constant.other.placeholder"],
			"settings": {
				"foreground": "#75a4f7"
			}
		},
		{
			"name": "Colors",
			"scope": ["constant.other.color"],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "#f13636"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"keyword.control",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#e659a9"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#DF8E43"
			}
		},
		{
			"name": "punctuation tag",
			"scope": [
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#ff76f6"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#ff76f6"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#45F070",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": ["meta.block variable.other"],
			"settings": {
				"foreground": "#82b1fc"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": ["support.other.variable", "string.other.link"],
			"settings": {
				"foreground": "#fa9b15"
			}
		},
		{
			"name": "Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#BD93F9"
			}
		},
		{
			"name": "Number",
			"scope": ["constant.numeric"],
			"settings": {
				"foreground": "#faa72a",
				"fontStyle": "bold"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#df8e43"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#3EC460"
			}
		},
		{
			"name": "Entity Types",
			"scope": ["support.type"],
			"settings": {
				"foreground": "#39daeb"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#39daeb"
			}
		},
		{
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Language methods",
			"scope": ["variable.language"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FF5370"
			}
		},
		{
			"name": "entity.name.method.js",
			"scope": ["entity.name.method.js"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#45F070"
			}
		},
		{
			"name": "meta.method.js",
			"scope": [
				"meta.class-method.js entity.name.function.js",
				"variable.function.constructor"
			],
			"settings": {
				"foreground": "#45F070",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Attributes",
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#3EC460"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#3EC460"
			}
		},
		{
			"name": "CSS Classes",
			"scope": ["entity.other.attribute-name.class"],
			"settings": {
				"foreground": "#3EC460"
			}
		},
		{
			"name": "CSS ID's",
			"scope": ["source.sass keyword.control"],
			"settings": {
				"foreground": "#45F070"
			}
		},
		{
			"name": "Inserted",
			"scope": ["markup.inserted"],
			"settings": {
				"foreground": "#df8e43"
			}
		},
		{
			"name": "Deleted",
			"scope": ["markup.deleted"],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Changed",
			"scope": ["markup.changed"],
			"settings": {
				"foreground": "#3EC460"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": ["string.regexp"],
			"settings": {
				"foreground": "#ff76f6"
			}
		},
		{
			"name": "Escape Characters",
			"scope": ["constant.character.escape"],
			"settings": {
				"foreground": "#ff76f6"
			}
		},
		{
			"name": "URL",
			"scope": ["*url*", "*link*", "*uri*"],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"tag.decorator.js entity.name.tag.js",
				"tag.decorator.js punctuation.definition.tag.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#45F070"
			}
		},
		{
			"name": "ES7 Bind Operator",
			"scope": [
				"source.js constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FF5370"
			}
		},
		{
			"name": "JSON Key - Level 0",
			"scope": [
				"source.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#39daeb"
			}
		},
		{
			"name": "JSON Key - Level 1",
